  "scripts": {
    "format": "biome format .",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "build": "nx run-many --target=build --all --configuration=development",
    "build:production": "nx run-many --target=build --all --configuration=production",
    "serve": "nx run-many --target=serve --all --exclude=daemon --configuration=development",
    "serve:skip-runner": "nx run-many --target=serve --all --exclude=runner,daemon --configuration=development",
    "serve:skip-proxy": "nx run-many --target=serve --all --exclude=proxy,daemon --configuration=development",
    "serve:production": "nx run-many --target=serve --all --exclude=daemon  --configuration=production",
    "db:generate": "cd apps/api && bunx ts-node -P ./tsconfig.json -r tsconfig-paths/register ../../node_modules/typeorm/cli.js migration:generate -d ./src/data-source.ts ./src/migrations/migration",
    "db:push": "cd apps/api && bunx ts-node -P ./tsconfig.json -r tsconfig-paths/register ../../node_modules/typeorm/cli.js migration:run -d ./src/data-source.ts",
    "openapi:generate": "nx run-many --target=openapi --all",
    "generate:api-client": "bun run openapi:generate && nx run-many --target=generate:api-client --all"
  },
